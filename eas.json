{
    "cli": {
      "version": ">= 12.5.1",
      "appVersionSource": "local"
    },
    "build": {
      "development-simulator": {
        "developmentClient": true,
        "distribution": "internal",
        "ios": {
          "simulator": true
        }
      },
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal",
        "channel": "preview"
      },
      "production": {
        "autoIncrement": true,
        "android": {
          "buildType": "app-bundle"
        },
        "channel": "production",
        "env": {
          "APP_ENV": "prod"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
  